<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.exportWizards">
      <category
            id="net.emmanuelsotelo.ProjectDow.projectDow"
            name="Project Dow">
      </category>
      <wizard
            category="net.emmanuelsotelo.ProjectDow.projectDow"
            class="net.emmanuelsotelo.projectdow.export.TIDataWizard"
            id="net.emmanuelsotelo.ProjectDow.export.TIDataWizard"
            name="Technical Indicator Data Export">
         <description>
            Export chart data generated by Technnical Indicators. Data is exported in CSV format.
         </description>
      </wizard>
   </extension>
   <extension
         point="net.sourceforge.eclipsetrader.charts.indicators">
      <group
            id="net.emmanuelsotelo.ProjectDow.indicators.group1"
            name="Project Dow">
      </group>
      <indicator
            class="net.emmanuelsotelo.projectdow.indicators.Weight"
            group="net.emmanuelsotelo.ProjectDow.indicators.group1"
            id="net.emmanuelsotelo.ProjectDow.indicators.Weight"
            name="Weight">
         <preferencePage
               class="net.emmanuelsotelo.projectdow.indicators.WeightPreferencePage"
               description="Prefernce page for the for Weight Indicator"
               title="Weight Preference Page">
         </preferencePage>
      </indicator>
      <indicator
            class="net.emmanuelsotelo.projectdow.indicators.CustomIndicator"
            group="net.emmanuelsotelo.ProjectDow.indicators.group1"
            id="net.emmanuelsotelo.ProjectDow.indicators.CustomIndicator"
            name="Custom Indicator">
         <preferencePage
               class="net.emmanuelsotelo.projectdow.indicators.CustomIndicatorPreferencePage"
               description="Customize the Indicator"
               title="Custom Indicator Setup">
         </preferencePage>
      </indicator>
      <indicator
            class="net.emmanuelsotelo.projectdow.indicators.ThreeDayDownTrend"
            group="net.emmanuelsotelo.ProjectDow.indicators.group1"
            id="net.emmanuelsotelo.ProjectDow.indicators.ThreeDayDownTrend"
            name="3-Day Down Trend">
         <preferencePage
               class="net.emmanuelsotelo.projectdow.indicators.ThreeDayDownTrendPreferencePage"
               title="3-Day Down Trend Preference Page">
         </preferencePage>
      </indicator>
      <indicator
            class="net.emmanuelsotelo.projectdow.indicators.ThreeDayUpTrend"
            group="net.emmanuelsotelo.ProjectDow.indicators.group1"
            id="net.emmanuelsotelo.ProjectDow.indicators.ThreeDayUpTrend"
            name="3-Day Up Trend">
         <preferencePage
               class="net.emmanuelsotelo.projectdow.indicators.ThreeDayUpTrendPreferencePage"
               title="3-Day Up Trend Preference Page">
         </preferencePage>
      </indicator>
   </extension>
   <extension
         point="net.sourceforge.eclipsetrader.trading.systems">
      <system
            class="net.emmanuelsotelo.projectdow.trading.CustomTradingSystem"
            id="net.emmanuelsotelo.ProjectDow.trading.CustomTradingSystem"
            name="Custom Trading System">
         <preferencePage
               class="net.emmanuelsotelo.projectdow.trading.CustomTradingSystemPreferencePage"
               name="Custom Trading System">
         </preferencePage>
      </system>
      <system
            class="net.emmanuelsotelo.projectdow.trading.IndicatorTradingSystem"
            id="net.emmanuelsotelo.projectdow.trading.IndicatorTradingSystem"
            name="Indicator Trading System">
         <preferencePage
               class="net.emmanuelsotelo.projectdow.trading.IndicatorTradingSystemPreferencePage"
               name="Indicator Trading System Preference Page">
         </preferencePage>
      </system>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            class="net.emmanuelsotelo.projectdow.views.NeuralPredictForwardView"
            id="net.emmanuelsotelo.ProjectDow.views.NeuralPredictForwardView"
            name="Stock Price Predictor">
      </view>
   </extension>

</plugin>
